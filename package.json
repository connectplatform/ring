{
  "name": "ring-platform-org",
  "version": "1.50",
  "private": true,
  "type": "module",
  "bin": {
    "ring": "./cli/index.js"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--no-deprecation' next dev",
    "dev:portal": "NODE_OPTIONS='--no-deprecation' next dev",
    "dev:ws": "NODE_OPTIONS='--no-deprecation' node server.js",
    "type-check": "tsc --noEmit",
    "build": "npm run type-check && NODE_OPTIONS='--no-deprecation' next build",
    "build:skip-types": "NODE_OPTIONS='--no-deprecation' next build",
    "build:portal": "NODE_OPTIONS='--no-deprecation' next build",
    "start": "next start",
    "start:portal": "PORT=3000 npm run start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache",
    "build:clean": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "setup": "node scripts/setup-env.js",
    "setup:env": "node scripts/setup-env.js",
    "setup:new": "node scripts/setup-env.js --force",
    "setup:portal": "cp .env.portal.template .env.local",
    "config:firebase": "node scripts/inject-firebase-config.js",
    "analyze": "ANALYZE=true npm run build:skip-types",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build:skip-types",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build:skip-types",
    "seed:opportunities": "node --experimental-modules scripts/seed-opportunities.js",
    "seed:portal": "node scripts/seed-portal-data.js",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "optimize:firebase": "node scripts/optimize-firebase-usage.js",
    "migrate:services": "node scripts/migrate-services-to-optimized.js",
    "fix:service-migration": "node scripts/fix-service-migration.js",
    "fix:compilation-errors": "node scripts/fix-remaining-compilation-errors.js",
    "test:firebase-optimization": "node scripts/test-firebase-optimization.js",
    "build:optimized": "BUILD_CACHE_DEBUG=true FIREBASE_DEBUG_LOGS=true npm run build && echo 'ðŸ“Š Check build logs for Firebase optimization results'",
    "build:analyze-performance": "BUILD_CACHE_DEBUG=true FIREBASE_DEBUG_LOGS=true time npm run build",
    "portal:health": "node scripts/check-portal-health.js",
    "portal:analytics": "node scripts/generate-portal-analytics.js",
    "cli:install": "cd cli && npm install && npm link",
    "cli:dev": "node cli/index.js"
  },
