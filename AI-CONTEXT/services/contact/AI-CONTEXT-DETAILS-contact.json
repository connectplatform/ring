{
  "schema_version": "1.0",
  "context_version": 1,
  "domain": "contact",
  "updated": "2025-08-09",
  "status": "PRODUCTION_READY",
  "owner": { "team": "platform-core", "maintainers": ["frontend@technoring"] },
  "purpose": "Submit and manage contact form submissions using Firestore Admin.",
  "files": {
    "service": "ring/services/contact/contact-service.ts"
  },
  "endpoints": [],
  "services": {
    "ContactService": {
      "file": "ring/services/contact/contact-service.ts",
      "methods": [
        { "name": "submitContactForm", "args": ["{ name, email, message }"], "returns": "Promise<void>" },
        { "name": "getContactFormSubmissions", "args": ["limit: number", "startAfter?: string"], "returns": "Promise<Array<{id:string,data:any}>>" }
      ]
    }
  },
  "storage": { "firestore_collections": ["contactForms", "errorLogs"] },
  "dependencies": { "intra_repo": ["ring/lib/firebase-admin.server.ts"], "env": ["AUTH_FIREBASE_PROJECT_ID", "AUTH_FIREBASE_CLIENT_EMAIL", "AUTH_FIREBASE_PRIVATE_KEY"] },
  "errors": [ { "code": "db_error", "when": "Firestore write/read failure" } ]
}


